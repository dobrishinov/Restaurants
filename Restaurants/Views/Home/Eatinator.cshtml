@model Restaurants.ViewModels.Restaurants.RestaurantsEditVM

<!-- Page Content -->
<div class="container">
    <div class="row">
        <div class="col-md-8 col-md-offset-2">
            <h2>Today best place for you is:</h2>
            <div class="thumbnail">
                <img class="img-responsive thumbnail" src="@Model.ImageUrl" alt="">
                <hr />
                <div class="caption-full">
                    <h4 class="pull-right"></h4>
                    <h4>
                        <i class="fa fa-cutlery fa-3x col-lg-2" aria-hidden="true"></i><a href="#">@Model.Name</a>
                    </h4>
                    <p>@Model.Type</p>
                    <p>
                        <hr />
                        @Model.Description
                    </p>
                </div>
                <hr />
                <ul class="list-group">
                    <li class="list-group-item list-group-item-success">
                        <i class="fa fa-clock-o" aria-hidden="true"></i>
                        Working Time: @Model.WorkingTime
                    </li>
                    <li class="list-group-item list-group-item-info">
                        <i class="fa fa-envelope-o" aria-hidden="true"></i>
                        @Model.Email
                    </li>
                    <li class="list-group-item list-group-item-warning"><i class="fa fa-map-marker" aria-hidden="true"></i> @Model.Address</li>
                    <li class="list-group-item list-group-item-danger"><i class="fa fa-phone" aria-hidden="true"></i> @Model.Phone</li>
                </ul>
                <div class="panel panel-default">
                    @*<iframe class="img-responsive" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2958.3662464629524!2d24.74101781576008!3d42.14244885680813!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14acd1b745fc08b5%3A0x4cd3205c60b5351d!2z0JDQt9C40Y8!5e0!3m2!1sbg!2sbg!4v1471954550094" width="800" height="600" frameborder="0" style="border:0" allowfullscreen></iframe>*@
                    <iframe class="img-responsive" width="800" height="600" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/place?key=AIzaSyDVTHPYwXZEiimTFLlMKQIDdxMoo-DDg4U&q=@Model.Address" allowfullscreen></iframe>
                </div>

            </div>

            <div class="well">
                <div class="row">
                    <div id="disqus_thread"></div>
                    <script>

/**
